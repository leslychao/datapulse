### Run ETL Scenario — ALL_EVENTS
POST {{host}}/api/etl/scenario/run
Content-Type: application/json

{
  "accountId": 6,
  "events": [
    {
      "event": "WAREHOUSE_DICT",
      "dateMode": "LAST_DAYS",
      "lastDays": 30
    },
    {
      "event": "CATEGORY_DICT",
      "dateMode": "LAST_DAYS",
      "lastDays": 30
    },
    {
      "event": "TARIFF_DICT",
      "dateMode": "LAST_DAYS",
      "lastDays": 30
    },
    {
      "event": "PRODUCT_DICT",
      "dateMode": "LAST_DAYS",
      "lastDays": 7
    },
    {
      "event": "SALES_FACT",
      "dateMode": "LAST_DAYS",
      "lastDays": 7
    },
    {
      "event": "INVENTORY_FACT",
      "dateMode": "LAST_DAYS",
      "lastDays": 7
    },
    {
      "event": "FACT_FINANCE",
      "dateMode": "LAST_DAYS",
      "lastDays": 7
    }
  ]
}

> {%
  client.test("ETL SCENARIO — ALL_EVENTS run", function () {
    client.assert(
        response.status === 200 || response.status === 202,
        "expected 200 or 202, got " + response.status
    );
  });
%}
